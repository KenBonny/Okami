{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "okami-freezer",
  "compatibility_date": "2025-08-15",
  "assets": {
    "directory": "./dist",
    "not_found_handling": "404-page"
  },
  "observability": {
    "enabled": false,
    "head_sampling_rate": 1,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "persist": true,
      "invocation_logs": true
    }
  }
}
